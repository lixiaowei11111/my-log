  "scripts": {
    "dev": "npm run lint:eslint:fix && npm run start:dev",
    "build": "npm run start:prod",
    "deploy": "cross-env NODE_ENV=production ROOT_PATH=/my-log/ npm run build ",
    "build:node": "cross-env NODE_ENV=production node --trace-warnings ./build/build.js",
    "build:report": "cross-env NODE_ENV=production npm_config_report=true npm run build:node",
    "start:dev": "cross-env NODE_ENV=development webpack serve --mode=development --progress --config ./build/webpack.config.dev.js",
    "start:staging": "cross-env NODE_ENV=staging webpack --mode=staging --config ./build/webpack.config.prod.js",
    "start:prod": "cross-env NODE_ENV=production webpack --mode=production --config ./build/webpack.config.prod.js",
    "start": "pm2 start app.js --name my-blog",
    "stop": "pm2 stop my-blog && pm2 delete my-blog",
    "status": "pm2 status",
    "logs": "pm2 logs",
    "test": "jest",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:stylelint": "",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "npx git-cz",
    "clean": "rm -rf node_modules",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
